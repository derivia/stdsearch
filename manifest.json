{
  "manifest_version": 3,
  "name": "Rust Search",
  "version": "1.1",
  "description": "Search Rust's STD docs & Crates from search bar using `!rust`",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "!rust"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "./icons/16.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  }
}
